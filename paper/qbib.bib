keywords = {Antigenic Variation,Antigenic Variation: immunology,Bordetella pertussis,Bordetella pertussis: genetics,Bordetella pertussis: immunology,Bordetella pertussis: physiology,Computer Simulation,Epidemiologic Methods,Immunologic Memory,Immunologic Memory: immunology,Models, Biological,Stochastic Processes,Whooping Cough,Whooping Cough: epidemiology,Whooping Cough: immunology,Whooping Cough: microbiology},
month = feb,
number = {1585},
pages = {409--16},
pmid = {16615206},
title = {{Integrating life history and cross-immunity into the evolutionary dynamics of pathogens.}},
