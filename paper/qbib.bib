journal = {Molecular biology and evolution},
keywords = {Adamantane,Adamantane: pharmacology,Amino Acid Substitution,Antiviral Agents,Antiviral Agents: pharmacology,Asia,Asia: epidemiology,Australia,Australia: epidemiology,Biological Assay,Drug Resistance, Viral,Drug Resistance, Viral: genetics,Evolution, Molecular,Genetic Variation,Genome, Viral,Humans,Influenza A Virus, H3N2 Subtype,Influenza A Virus, H3N2 Subtype: drug effects,Influenza A Virus, H3N2 Subtype: genetics,Influenza, Human,Influenza, Human: drug therapy,Influenza, Human: epidemiology,Influenza, Human: virology,Japan,Japan: epidemiology,New Zealand,New Zealand: epidemiology,Phylogeny,United States,United States: epidemiology,Viral Matrix Proteins,Viral Matrix Proteins: genetics},
